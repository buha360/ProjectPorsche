<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Car Bean -->
    <bean id="car" class="com.wardanger.ProjectPorsche.model.Car">
        <property name="make" value="Porsche"/>
        <property name="model" value="911"/>
        <property name="year" value="2021"/>
        <property name="user" ref="user"/> <!-- HivatkozÃ¡s a User Bean-re -->
    </bean>

</beans>
